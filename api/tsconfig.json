{
    "compilerOptions": {
      "target": "es2017", 
      "module": "esnext", 
      "lib": ["es2020", "dom"], 
      "strict": true,
      "moduleResolution": "node", 
      "esModuleInterop": true,
      "skipLibCheck": true, 
      "forceConsistentCasingInFileNames": true, 
      "isolatedModules": true, 
      "resolveJsonModule": true, 
      "types": ["node"] 
    },
  }
  